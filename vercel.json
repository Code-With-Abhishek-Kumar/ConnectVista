{
  "version": 2,
  "builds": [
    { "src": "app.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node"
    }
  },
  "scripts": {
    "dev": "nodemon ./bin/www"
  }
}