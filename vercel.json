{
  "version": 2,
  "builds": [
    { "src": "app.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node"
    }
  },
  "devCommand": "nodemon ./bin/www",
  "cleanUrls": true,
  "public": true,
  "installCommand": "npm install"

}